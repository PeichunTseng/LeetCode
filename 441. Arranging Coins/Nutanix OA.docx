////////////////////////////Mine//////////////////////////////
    public int arrangeCoins(int n) {
        int res=(int)Math.sqrt(2*(long)n);
        if(res*(res+1)>2*n)
            res--;
        return res;
    }

    public static void main(String[] args){
       tryyy tr=new tryyy(); 
       int n=8;
       int ans=tr.arrangeCoins(n);
       System.out.println(ans);           
    }
}